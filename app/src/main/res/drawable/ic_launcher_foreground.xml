<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- קבוצה שמרכזת את הציור בתוך הקנבס -->
    <group
        android:translateX="-6"
        android:translateY="14">

        <!-- ידית המפתח: עיגול עם חלל פנימי (באמצעות evenOdd) -->
        <path
            android:fillColor="#000000"
            android:fillType="evenOdd"
            android:pathData="
                M20,40
                A15,15 0 1,1 50,40
                A15,15 0 1,1 20,40 Z
                M30,40
                A5,5 0 1,0 40,40
                A5,5 0 1,0 30,40 Z" />

        <!-- גוף המפתח (Shaft): מלבן המחובר לידית -->
        <path
            android:fillColor="#000000"
            android:pathData="M50,35 H90 V45 H50 Z" />

        <!-- שן המפתח: קצה ימני מעודן עם קימור עדין -->
        <path
            android:fillColor="#000000"
            android:pathData="M90,35
                              C96,32, 100,35, 100,40
                              C100,45, 96,48, 90,45 Z" />

    </group>
</vector>
